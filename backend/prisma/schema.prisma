generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  name String 
  email String
  password String 
  profilePhoto String 
  tracks Track[]
}

model Track {
  id Int @id @default(autoincrement())
  userId Int 
  user User @relation(fields: [userId], references: [id])
}

